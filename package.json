{
  "name": "@theponti/ponti-ui",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "build:ci": "pnpm install && pnpm test:ci && tsc && vite build",
    "commit": "cz",
    "db:restart": "supabase stop && supabase start",
    "dev": "vite --open",
    "lint": "run-p run-tsc run-eslint run-stylelint",
    "prepare": "husky install",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "refresh-deps": "rm pnpm-lock.yaml && rm -rf node_modules && pnpm install",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx,.js,.md src && prettier -w src",
    "run-eslint:time": "TIMING=1 eslint src",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore \"**/*.scss\"",
    "run-tsc": "tsc",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "open coverage/lcov-report/index.html",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:update": "vitest run -u",
    "validate": "run-p lint test:ci test:e2e:headless"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "@supabase/supabase-js": "2.0.4",
    "@testing-library/react": "13.4.0",
    "axios": "1.1.3",
    "classnames": "^2.3.1",
    "daisyui": "^2.36.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "6.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@cypress/code-coverage": "3.10.0",
    "@nabla/vite-plugin-eslint": "1.4.1",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/css-mediaquery": "0.1.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-router-dom": "5.3.3",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vitest/ui": "^0.24.3",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.0.0",
    "c8": "7.12.0",
    "commitizen": "4.2.5",
    "css-mediaquery": "0.1.2",
    "cypress": "10.11.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-prefer-function-component": "3.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "husky": "8.0.1",
    "jsdom": "20.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.18",
    "prettier": "^2.7.1",
    "rollup": "3.2.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.55.0",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.14.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "tailwindcss": "^3.2.1",
    "typescript": "4.8.4",
    "vite": "3.2.1",
    "vite-plugin-pwa": "0.13.1",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.24.3",
    "whatwg-fetch": "3.6.2",
    "workbox-build": "6.5.4",
    "workbox-window": "6.5.4"
  },
  "browserslist": {
    "production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "lint-staged": {
    "*": "prettier -w",
    "*.css": "stylelint --cache --fix",
    "*.{ts,tsx}": [
      "pnpm run-eslint",
      "vitest related --run --coverage=false"
    ]
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "reporter": [
      "lcov"
    ]
  }
}
